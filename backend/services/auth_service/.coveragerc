[run]
source = app
omit =
    */tests/*
    */test_*.py
    */__init__.py
    */migrations/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    @abstractmethod
    @abc.abstractmethod
    if TYPE_CHECKING:

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
